{
  "name": "mini-tools",
  "description": "mini tools for express and others",
  "version": "1.0.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": "codenautas/mini-tools",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "dependencies": {
    "@types/express": "~4.11.1",
    "@types/fs-extra": "~5.0.1",
    "@types/js-yaml": "~3.10.1",
    "@types/node": "~9.4.7",
    "@types/send": "~0.14.4",
    "fs-extra": "~5.0.0",
    "js-yaml": "~3.11.0",
    "pug": "~2.0.1",
    "read-yaml-promise": "~1.0.2",
    "send": "~0.16.2",
    "stylus": "~0.54.5",
    "best-globals": "~0.10.4"
  },
  "devDependencies": {
    "@types/mocha": "~2.2.48",
    "colors": "~1.1.2",
    "expect.js": "~0.3.1",
    "express": "~4.16.2",
    "istanbul": "~0.4.5",
    "mocha": "~3.5.3",
    "nyc": "~11.4.1",
    "sinon": "~4.4.2",
    "source-map-support": "~0.5.3",
    "supertest": "~3.0.0",
    "ts-node": "~5.0.1",
    "typescript": "~2.7.2"
  },
  "main": "lib/mini-tools.js",
  "types": "lib/mini-tools.d.ts",
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "node node_modules/nyc/bin/nyc mocha",
    "start": "node example/server.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "designing",
    "coverage": 100
  }
}
