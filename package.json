{
  "name": "mini-tools",
  "description": "mini tools for express and others",
  "version": "1.0.3",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": "codenautas/mini-tools",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "dependencies": {
    "fs-extra": "~7.0.0",
    "js-yaml": "~3.12.0",
    "pug": "~2.0.3",
    "read-yaml-promise": "~1.0.2",
    "send": "~0.16.2",
    "stylus": "~0.54.5",
    "best-globals": "~0.10.5"
  },
  "devDependencies": {
    "@types/express": "~4.16.0",
    "@types/fs-extra": "~5.0.4",
    "@types/js-yaml": "~3.11.2",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.5.2",
    "@types/send": "~0.14.4",
    "colors": "~1.3.0",
    "expect.js": "~0.3.1",
    "express": "~4.16.3",
    "mocha": "~5.2.0",
    "nyc": "~12.0.2",
    "sinon": "~6.1.3",
    "source-map-support": "~0.5.6",
    "supertest": "~3.1.0",
    "ts-node": "~7.0.0",
    "typescript": "~2.9.2"
  },
  "main": "lib/mini-tools.js",
  "types": "lib/mini-tools.d.ts",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --reporter spec --bail --check-leaks --exit test/",
    "test-ci": "node node_modules/nyc/bin/nyc --reporter=lcov --reporter=text-lcov node_modules/mocha/bin/_mocha --exit --check-leaks test/",
    "test-cov": "node node_modules/nyc/bin/nyc mocha --exit",
    "start": "node example/server.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "lib"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "designing",
    "coverage": 100
  }
}
