{
  "name": "mini-tools",
  "description": "mini tools for express and others",
  "version": "1.10.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": "codenautas/mini-tools",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "dependencies": {
    "fs-extra": "^9.0.0",
    "js-yaml": "^3.14.0",
    "pug": "^3.0.0",
    "read-yaml-promise": "^1.0.2",
    "send": "~0.17.1",
    "stylus": "~0.54.7",
    "best-globals": "~0.10.24"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/js-yaml": "^3.12.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/send": "~0.14.5",
    "colors": "^1.4.0",
    "expect.js": "~0.3.1",
    "express": "^4.17.1",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "source-map-support": "~0.5.19",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "typescript": "~3.9.3"
  },
  "main": "lib/mini-tools.js",
  "types": "lib/mini-tools.d.ts",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --reporter spec --bail --check-leaks --exit test/",
    "test-ci": "node node_modules/nyc/bin/nyc --reporter=lcov --reporter=text-lcov node_modules/mocha/bin/_mocha --exit --check-leaks test/",
    "test-cov": "node node_modules/nyc/bin/nyc mocha --exit",
    "start": "node example/server.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "lib"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "designing",
    "coverage": 100
  }
}
